package com.zak.isp.example.good;


public class Main {

	public static void main(String[] args) {
		
		IClock icClock=new SimpleClock();
		System.out.println(icClock.getTime());
		
		icClock=new AlarmClock();
		icClock.setAlarm(10);
		icClock.startAlarm();
		System.out.println("Alarm completed");
	}

}
