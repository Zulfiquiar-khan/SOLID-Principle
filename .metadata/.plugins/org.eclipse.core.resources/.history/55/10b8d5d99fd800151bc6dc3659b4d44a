package com.zak.bad.example.good.two;

public class Graph2D {
	
	private int x;
	private int y;
	
	
	
	public int getX() {
		return x;
	}



	public void setX(int x) {
		this.x = x;
	}



	public int getY() {
		return y;
	}



	public void setY(int y) {
		this.y = y;
	}



	public void draw(){
		System.out.println("Drawing 2D graph at "+x+","+y);
	}
}
